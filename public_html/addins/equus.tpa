<script LANGUAGE='javascript'>


var video = document.getElementById('equusvid');

function togglePlay()
{
    if(video.paused)
    {
        video.play();
        document.getElementById('toggleimg').src='/assets/pauze.png';
    }
    else
    {
        video.pause();
        document.getElementById('toggleimg').src='/assets/play.png';
    }
}

var videodiv = document.getElementById('video');
videodiv.onmouseover = function(){document.getElementById('video-overlay').style.visibility = 'visible';};
videodiv.onmouseout = function(){document.getElementById('video-overlay').style.visibility='hidden';};
videodiv.onclick = function(){togglePlay();};

video.canplaythrough = video.play();
video.load();
video.canplaythrough = video.play();


</script>
